// Find the factorial of a number

#include<bits/stdc++.h>
using namespace std;

int factorial(int n){
    int fact=1;
    for(int i=2;i<=n;i++){
        fact=fact*i;
    }
    return fact;
}

int main(){
    int n;
    cout<<"Enter a number : ";
    cin>>n;
    cout<<"The factcorial of "<<n<<" is "<<factorial(n)<<endl;
    return 0;
}
